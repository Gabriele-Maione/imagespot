   <children>
      <Button fx:id="choose" layoutX="52.0" layoutY="304.0" mnemonicParsing="false" prefHeight="46.0" prefWidth="133.0" text="choose photo" onAction="#onBtnLoadImageClick"/>
      <Button fx:id="add" layoutX="300.0" layoutY="300.0" mnemonicParsing="false" prefHeight="54.0" prefWidth="90.0" text="ADD" onAction="#onBtnAddImageClick"/>
      <ImageView fx:id="img" fitHeight="203.0" fitWidth="289.0" layoutX="156.0" layoutY="39.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
